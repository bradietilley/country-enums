{
    "name": "bradietilley/country-enums",
    "description": "All (or at least most) countries and their regions formatted as PHP enums",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CountryEnums\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Bradie Tilley",
            "homepage": "https://github.com/bradietilley"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.6",
        "pestphp/pest-plugin-laravel": "^2.0",
        "laravel/pint": "^1.10",
        "rector/rector": "^0.16.0"
    },
    "require": {
        "php": "^8.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
